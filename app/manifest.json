{
  "manifest_version": 2,

  "name": "Battlecode games",
  "description": "This extension shows how many games opened on codebattle.io.",
  "version": "0.2.2",

  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "lib/jquery.min.js",
      "lib/bullet.js",
      "lib/lodash.min.js",
      "scripts/build/background.js"
   ]
  }
}
